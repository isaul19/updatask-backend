{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": true
    },

    "transform": null,
    "target": "es2016",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "baseUrl": "./",
    "paths": {
      "@boostrap/*": ["./src/boostrap/*"],
      "@adapters/*": ["./src/config/adapters/*"],
      "@lib/*": ["./src/config/lib/*"],
      "@errors/*": ["./src/config/errors/*"],
      "@routers/*": ["./src/routers/*"],
      "@controllers/*": ["./src/controllers/*"],
      "@config/*": ["./src/config/*"],
      "@services/*": ["./src/services/*"],
      "@models/*": ["./src/models/*"],
      "@dtos/*": ["./src/dtos/*"],
      "@middlewares/*": ["./src/middlewares/*"],
      "@repositories/*": ["./src/repositories/*"]
    }
  },
  "module": {
    "type": "commonjs",
    "strict": true,
    "strictMode": true,
    "noInterop": false,
    "lazy": false
  },
  "minify": true
}
